{
  "name": "react-boilerplate",
  "version": "4.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=8",
    "node": ">=18"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color --progress",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "eslint:fix": "node node_modules/eslint/bin/eslint.js app/ --fix --max-warnings 0",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "antd": "^4.24.7",
    "axios": "^0.26.1",
    "c3": "^0.4.24",
    "chalk": "^4.1.2",
    "compression": "1.7.4",
    "connected-react-router": "6.9.1",
    "cookie": "^0.4.2",
    "cross-env": "7.0.3",
    "d3": "^7.8.2",
    "dompurify": "^2.4.3",
    "escape-html": "^1.0.3",
    "express": "^4.18.2",
    "fontfaceobserver": "2.1.0",
    "history": "4.9.0",
    "hoist-non-react-statics": "3.3.2",
    "http-proxy-middleware": "^2.0.6",
    "immer": "^9.0.18",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "less": "^4.1.3",
    "less-loader": "^10.2.0",
    "lodash": "^4.17.21",
    "marked": "^4.2.12",
    "md5": "^2.3.0",
    "minimist": "^1.2.7",
    "polished": "^4.2.2",
    "prop-types": "15.7.2",
    "punycode": "^2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-force-graph-2d": "^1.23.15",
    "react-grid-layout": "^1.3.4",
    "react-helmet": "6.1.0",
    "react-hotkeys-hook": "^3.4.7",
    "react-items-carousel": "^2.8.0",
    "react-json-view": "^1.21.3",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-simple-maps": "^3.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^4.5.1",
    "react-transition-group": "^4.4.5",
    "redux": "4.1.0",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "store": "^2.0.12",
    "styled-components": "5.3.0",
    "webpack-bundle-tracker": "^1.8.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.14.5",
    "@babel/plugin-transform-react-constant-elements": "7.14.5",
    "@babel/plugin-transform-react-inline-elements": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/register": "7.14.5",
    "add-asset-html-webpack-plugin": "^5.0.2",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.2.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-import": "^1.13.6",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-styled-components": "1.12.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "circular-dependency-plugin": "5.2.2",
    "compare-versions": "3.6.0",
    "compression-webpack-plugin": "^8.0.1",
    "coveralls": "3.1.0",
    "css-loader": "5.2.6",
    "dotenv-webpack": "^8.0.1",
    "eslint": "7.29.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-redux-saga": "1.2.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "imports-loader": "3.0.0",
    "jest-cli": "27.0.5",
    "jest-dom": "3.1.3",
    "jest-styled-components": "7.0.4",
    "lint-staged": "11.0.0",
    "node-plop": "0.26.2",
    "null-loader": "4.0.1",
    "plop": "2.7.4",
    "pre-commit": "1.2.2",
    "prettier": "2.3.2",
    "react-app-polyfill": "2.0.0",
    "react-test-renderer": "17.0.2",
    "react-testing-library": "6.1.2",
    "redux-logger": "^3.0.6",
    "rimraf": "3.0.2",
    "shelljs": "^0.8.5",
    "style-loader": "3.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-middleware": "5.0.0",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-pwa-manifest": "4.3.0",
    "whatwg-fetch": "3.6.2"
  }
}
